Kt = 0;
l = 5;
d = 0.089;
r = 0.0325;


R = 4.3111;
M = 2.34;
J = 0.0169;
Bv = 0.4978;
Bvn = 0.6763;
Bw = 0.0141;


Cv = 1.8738;
Cvn = 2.2198;
Cw = 0.1385;

A11 = (2*Kt^2*l^2)/(r^2*R*M) - Bv/M;
A22 = (-2*Kt^2*l^2)/(r^2*R*M) - Bvn/M;
A33 = (-4*d^2*Kt^2*l^2)/(r^2*R*J) - Bw/J;

A = [A11 0 0;
     0 A22 0;
     0 0 A33];

B = (l*kt)/(r*R) * [0 -1/M 0 1/M;
                    1/M 0 -1/M 0;
                    d/J d/J d/J d/J];
                
K = [-Cv/M 0 0;
    0 -Cvn/M 0;
    0 0 -Cw/J];
    